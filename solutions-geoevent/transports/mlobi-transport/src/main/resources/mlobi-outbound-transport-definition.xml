<transport name="MLOBIOutboundTransport" label="${com.esri.geoevent.solutions.transport.mlobi.mlobi-transport.TRANSPORT_LABEL}" domain="com.esri.geoevent.solutions.transport.mlobi.outbound" type="outbound">
	<description>${com.esri.geoevent.solutions.transport.mlobi.mlobi-transport.TRANSPORT_DESCRIPTION}</description>
	<propertyDefinitions>
		<propertyDefinition propertyName="server" label="${com.esri.geoevent.solutions.transport.mlobi.mlobi-transport.LBL_SERVER}"
			description="${com.esri.geoevent.solutions.transport.mlobi.mlobi-transport.DESC_SERVER}"
			propertyType="String" mandatory="true" readOnly="false" />
		<propertyDefinition propertyName="username" label="${com.esri.geoevent.solutions.transport.mlobi.mlobi-transport.LBL_USER}"
			description="${com.esri.geoevent.solutions.transport.mlobi.mlobi-transport.DESC_USER}"
			propertyType="String" mandatory="true" readOnly="false" />
		<propertyDefinition propertyName="password" label="${com.esri.geoevent.solutions.transport.mlobi.mlobi-transport.LBL_PW}"
            description="${com.esri.geoevent.solutions.transport.mlobi.mlobi-transport.DESC_PW}"
            propertyType="Password" mandatory="true"
            readOnly="false">
        </propertyDefinition>
		<propertyDefinition propertyName="featureservice" label="${com.esri.geoevent.solutions.transport.mlobi.mlobi-transport.LBL_SERVICE}"
            description="${com.esri.geoevent.solutions.transport.mlobi.mlobi-transport.DESC_SERVICE}"
            propertyType="String" mandatory="true"
            readOnly="false">
        </propertyDefinition>
        <propertyDefinition propertyName="layerindex" label="${com.esri.geoevent.solutions.transport.mlobi.mlobi-transport.LBL_INDEX}"
            description="${com.esri.geoevent.solutions.transport.mlobi.mlobi-transport.DESC_INDEX}"
            propertyType="Integer" mandatory="true"
            readOnly="false">
        </propertyDefinition>	
	</propertyDefinitions>
</transport>